{
    "name": "lextira/pureftpd-manager",
    "description": "Client library for https://github.com/lextira/pureftpd.",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.2.5",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/config": "^8.0|9.0",
        "illuminate/support": "^8.0|^9.0",
        "illuminate/validation": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Lextira\\PureFTPdManager\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Lextira\\PureFTPdManager\\Providers\\PureFTPdServiceProvider"
            ]
        }
    }
}
